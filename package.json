{
  "name": "311",
  "version": "0.0.0",
  "repository": "https://github.com/CityOfBoston/311.git",
  "license": "CC0-1.0",
  "engines": {
    "node": "7.5.0"
  },
  "scripts": {
    "dev": "rakt pages/index.js",
    "test": "jest",
    "build": "./scripts/build_server.sh",
    "heroku-postbuild": "./scripts/build_server.sh",
    "start": "node build/server",
    "storybook": "start-storybook -p 9001 -c storybook",
    "lint": "eslint --fix .",
    "generate-schema": "babel-node ./scripts/generate-schema",
    "generate-graphql-flow": "apollo-codegen generate data/graphql/*.graphql --schema graphql/schema.json --target flow --output data/graphql/schema.flow.js",
    "clear-babel-cache": "rm -rf node_modules/.cache/babel-loader",
    "precommit": "eslint --quiet .",
    "prepush": "flow check --quiet && jest --silent --no-cache --noStackTrace"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/.next/",
      "<rootDir>/coverage/"
    ],
    "collectCoverageFrom": [
      "{components,data,lib,pages,server}/**/*\\.js",
      "!**/*.stories.js"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "fs-extra": "^2.0.0",
    "glamor": "^2.20.22",
    "https-proxy-agent": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "rakt": "^0.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router-dom": "next",
    "redux": "^3.6.0",
    "redux-inject": "^1.0.2",
    "redux-pack": "^0.1.4",
    "redux-thunk": "^2.2.0",
    "urlsearchparams": "^0.1.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-watch": "^2.0.6",
    "codecov": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.39.0",
    "husky": "^0.13.1",
    "jest": "^18.1.0",
    "react-test-renderer": "^15.4.2",
    "webpack-bundle-analyzer": "^2.3.0"
  }
}
